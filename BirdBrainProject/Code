public class Finch{
	public static void main(String[] args) {
		Finch f = new Finch();
}
	public boolean isShaking()
		System.out.print(true);



// if the finch is shaking, means the ball is in the cup
//                          if not its not in the cup
// if the finch shakes, the finch should move to a random spot
		
		
}

// finch move code:

public class example{
	public static void main(String[] args) { 

	//system.out.println();
	
	Finch f = new Finch();
	
	f.setMotors(10,10);
	f.pause(3);
	f.stop();
	int t = (int)(Math.random() * 360) + 1;
	f.setTurn("R" , "t", 50);
	System.out.println(t);
	f.stop();
	
}
